# ZebraStream — Copy to .env and fill in values
# Never commit .env to git

# PostgreSQL (required)
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=zebrastream
POSTGRES_USER=zebra_app
POSTGRES_PASSWORD=your_secure_password_here

# OpenAI (optional — for AI maintenance summary)
OPENAI_API_KEY=

# Testing (set by pytest, do not change)
# TESTING=0
